#ifndef VERSION_H

/*
  This is where current version number is stored
  These values may be edited by hand
  cmake uses these version numbers to populate the rest of this file
  cmake will increment the patch number for every build if build option 
  -DSKIP_AUTO_VERSION=OFF 

  VERSION_RELEASE 0
  VERSION_MAJOR 1
  VERSION_MINOR 0
  VERSION_PATCH 20
*/

// ****** DO NOT EDIT THIS FILE BELOW THIS POINT *********
// ****** DO NOT EDIT THIS FILE BELOW THIS POINT *********
// ****** DO NOT EDIT THIS FILE BELOW THIS POINT *********

#define VERSION_RELEASE "@_version_release@"
#define VERSION_MAJOR "@_version_major@"
#define VERSION_MINOR "@_version_minor@"
#define VERSION_PATCH "@_version_patch@"

#define VERSION_RELEASE_I @_version_release@
#define VERSION_MAJOR_I @_version_major@
#define VERSION_MINOR_I @_version_minor@
#define VERSION_PATCH_I @_version_patch@

#define THISBUILD VERSION_RELEASE "." VERSION_MAJOR "." VERSION_MINOR "." VERSION_PATCH

#define FIRMWARE_VERSION VERSION_RELEASE_I, VERSION_MAJOR_I, VERSION_MINOR_I, VERSION_PATCH_I

#define COMPILE_DATE __DATE__

#define COMPILE_TIME __TIME__

#define COMPILER_VERSION __VERSION__

#define GIT_REVISION "@_commit_hash@"
#define COMMIT_TIMESTAMP "@_commit_date@"
#define BUILD_BRANCH "@_build_branch@"
#define BUILT_BY "@_user_name@"

// ****** DO NOT EDIT THIS FILE *********
// ****** DO NOT EDIT THIS FILE *********
// ****** DO NOT EDIT THIS FILE *********

#endif // VERSION_H
